  "dependencies": {
    "ask-sdk": "^2.1.0",
    "axios": "^0.18.0",
    "lambda-local": "^1.5.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.7.0",
