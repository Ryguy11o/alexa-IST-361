{
  "name": "psuhelper",
  "version": "1.0.0",
  "description": "Happy Valley helper - aws function",
  "main": "lambda.js",
  "scripts": {
    "pretest": "eslint --ignore-path .gitignore .",
    "fix": "./node_modules/.bin/eslint --fix --ignore-path .gitignore .",
    "test": "npm run academic_test && npm run cata_test",
